#X3D V3.2 utf8

PROFILE Interchange

DEF Transf Transform {
  children Inline {
    url "test_room.gltf"
  }
}

PROTO UpperLight [
  inputOutput SFVec3f lightPos 0 0 0
] {
  SpotLight {
    global TRUE
    location IS lightPos
    cutOffAngle 1.25
    direction 0 -1 0
    intensity 3
    ambientIntensity 0.4
    radius 500
    beamWidth 0.1
    shadows TRUE
    defaultShadowMap GeneratedShadowMap {
      size 1024
      update "ALWAYS"
      scale 10
    }
  }
}

PROTO BottomLight [
  inputOutput SFVec3f lightPos 0 0 0
] {
  PointLight {
    global TRUE
    location IS lightPos
    intensity 0.03
    ambientIntensity 0.4
    radius 500
  }
}

UpperLight { lightPos -49.21 80.38 16.4042 }
